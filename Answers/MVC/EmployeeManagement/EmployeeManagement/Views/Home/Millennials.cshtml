@model IEnumerable<Application.Models.MillennialDto>



<input type="button" value="Export" onclick="location.href='@Url.Action("Export", "Home",new { fileName = "Millennials"})'" />


<table class="table">
    <thead>
        <tr>
            <th>
                First Name
            </th>
            <th>
                Last Name
            </th>

            <th>
                IsMillennial
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IsMillennial)
                </td>

            </tr>
        }
    </tbody>
</table>
